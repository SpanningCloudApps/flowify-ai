{
  "name": "worker-cli",
  "description": "Execute defined workflows",
  "version": "1.0.0",
  "bin": {
    "worker-cli": "./bin/run"
  },
  "homepage": "https://github.com/SpanningCloudApps/hackathon-ai",
  "license": "UNLICENSED",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/SpanningCloudApps/hackathon-ai"
  },
  "engines": {
    "node": "^18.0.0"
  },
  "scripts": {
    "clean": "rm -rf lib coverage cli",
    "build": "npm run clean && tsc"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "2.15.0"
  },
  "oclif": {
    "bin": "worker-cli",
    "commands": "./lib/commands",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
